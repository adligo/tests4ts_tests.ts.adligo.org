    "npm"
  ],
  "devDependencies": {
    "@types/node": "^22.15.21",
    "child_process": "^1.0.2",
    "typescript": "^5.0.4"
  },
  "homepage": "https://github.com/adligo/tests4ts_tests.ts.adligo.org#readme",
  "keywords": [
    "symbolic",
    "link",
    "linking",
    "shared",
    "modules"
  ],
  "license": "Apache-2.0",
  "name": "@ts.adligo.org/slink",
  "main": "./src/slink.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adligo/tests4ts_tests.ts.adligo.org.git"
  },
  "scripts": {
    "build": "node build.cjs --install-local",
    "test": "test",
    "build": "tsc"
  },
  "sharedNodeModuleProjectSLinkEnvVar": [
    "TESTS4TS_NODE_MODULE_SLINK"
  ],
  "sharedNodeModuleProjectSLinks": [
